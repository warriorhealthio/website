extends ../layouts/default.pug

block config

    //- Global Page Configuration
    - var pageTitle = 'Warrior'


block content

    //- * * * * * * * *
    //- * * Navbar  * *
    //- * * * * * * * *
    include mixins/navbar.pug
    +navbar({
        navbarBg: 'bg-white',
        navbarStyle: 'navbar-light',
        navbarBrandColor: 'text-dark',
        navbarBtnColor: 'btn-primary',
        navbarContainer: 'container'
    })


    include mixins/header.pug
    +header({
        svgBorderFill: 'text-dark',
        pageHeaderBg: 'bg-white',
        pageHeaderStyle: 'page-header-white'
    })
        .page-header-content
            .container
                .row.align-items-center
                    .col-lg-7
                        h1.page-header-title Care for the athlete in everyone
                        p.page-header-text.mb-5 Recover from overuse injuries and prevent them from happening. Your new training and performance companion
                    .col-lg-5
                        img.img-fluid(src='assets/img/individuals-page/runner.jpg')        
                .row.pt-4
                    .col-lg-4.mb-5.mb-lg-0
                        .card.h-100
                            .card-body.p-5
                                .text-center
                                    img.img-fluid(src='assets/img/heal-and-prevent.png', width="60px").mb-2
                                    .h1 Heal and prevent overuse injuries
                                    .p.text-dark Repetitive movement and overuse can cause pain and damage. Recover and perform using Warrior

                    .col-lg-4.mb-5.mb-lg-0
                        .card.h-100
                            .card-body.p-5
                                .text-center
                                    img.img-fluid(src='assets/img/telemedicine-for-athletes.png', width="60px").mb-2
                                    .h1 Telehealth for athletes
                                    .p.text-dark Meet with a sports medicine specialist on your own schedule, anywhere, for virtual care

                    .col-lg-4.mb-5.mb-lg-0
                        .card.h-100
                            .card-body.p-5
                                .text-center
                                    img.img-fluid(src='assets/img/in-person.png', width="60px").mb-2
                                    .h1 Need to see someone in-person?
                                    .p.text-dark Referrals to verified sports medicine physicians in our network when an in-person visit is required

        .page-header-content.pt-4
            .container.pt-4
                .row.align-items-center.text-center.pt-4
                    .col-lg-12
                        h1 Sign up to learn how Warrior can help you
                .row.align-items-center.pt-4
                    .col-lg-2
